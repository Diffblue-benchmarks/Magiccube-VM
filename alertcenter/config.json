    "process": {
        "file_limit": 1000000,
        "core_limit": 1000000000
    },
    "log": {
        "path": "/log/alertcenter",
        "flag": "fatal,error,warn,info,debug"
    },
    "svr": {
        "bind": [
            "tcp@0.0.0.0:4444",
            "udp@0.0.0.0:4444"
        ],
        "max_pkg_size": 10240
    },
    "redis": {
        "name": "mymaster",
        "addr": [
            "exchange.magiccube.io:27379",
            "exchange.magiccube.io:27380",
            "exchange.magiccube.io:27381"
        ]
    }
}
